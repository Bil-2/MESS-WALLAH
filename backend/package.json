{
  "name": "mess-wallah-backend",
  "version": "1.0.0",
  "description": "Backend API for MESS WALLAH - Room booking platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node seedRooms.js",
    "mongo:start": "chmod +x ../start-mongodb.sh && ../start-mongodb.sh",
    "mongo:stop": "brew services stop mongodb/brew/mongodb-community",
    "mongo:restart": "npm run mongo:stop && npm run mongo:start",
    "mongo:status": "brew services list | grep mongodb",
    "dev:mongo": "npm run mongo:start && npm run dev",
    "start:mongo": "npm run mongo:start && npm start",
    "production:check": "node productionStartup.js",
    "production:start": "npm run production:check && npm run mongo:start && npm start",
    "production:full": "npm run production:check && npm run mongo:start && npm run seed && npm start",
    "api:test": "node final100PercentTest.js",
    "health:check": "curl -f http://localhost:5001/health || exit 1",
    "startup:complete": "npm run production:check && npm run mongo:start && npm run seed && npm start && npm run api:test"
  },
  "keywords": [
    "room-booking",
    "mess",
    "accommodation",
    "students",
    "india"
  ],
  "author": "MESS WALLAH Team",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.6",
    "razorpay": "^2.9.2",
    "twilio": "^4.23.0",
    "winston": "^3.10.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
