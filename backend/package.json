{
  "name": "mess-wallah-backend",
  "version": "1.0.0",
  "description": "Backend API for MESS WALLAH - Room booking platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node seedRooms.js",
    "mongo:start": "chmod +x ../start-mongodb.sh && ../start-mongodb.sh",
    "mongo:stop": "brew services stop mongodb/brew/mongodb-community",
    "mongo:restart": "npm run mongo:stop && npm run mongo:start",
    "mongo:status": "brew services list | grep mongodb",
    "dev:mongo": "npm run mongo:start && npm run dev",
    "start:mongo": "npm run mongo:start && npm start"
  },
  "keywords": [
    "room-booking",
    "mess",
    "accommodation",
    "students",
    "india"
  ],
  "author": "MESS WALLAH Team",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "mongoose-paginate-v2": "^1.8.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "socket.io": "^4.7.2",
    "twilio": "^4.15.0",
    "@sendgrid/mail": "^7.7.0",
    "cookie-parser": "^1.4.6",
    "uuid": "^9.0.0",
    "moment": "^2.29.4",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.5"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}